{
  "name": "most-js-client",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "generate-runtime": "bunx openapicmd read --strip openapi_client_axios --format json openapi.json > src/openapi-runtime.json",
    "generate-types": "bunx openapicmd typegen ./openapi.json > src/openapi.ts",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "openapi-client-axios": "^7.5.5"
  },
  "devDependencies": {
    "openapicmd": "^2.6.1",
    "typescript": "^5.6.3"
  }
}